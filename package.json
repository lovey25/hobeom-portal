{
  "name": "hobeom-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init": "bash scripts/init.sh",
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:push\" --names \"NEXT,PUSH\" --prefix-colors \"cyan,magenta\"",
    "dev:next": "NODE_ENV=development next dev --turbopack",
    "dev:push": "NODE_ENV=development nodemon scripts/push-scheduler.js",
    "start": "concurrently \"npm run prod:next\" \"npm run prod:push\" --names \"NEXT,PUSH\" --prefix-colors \"cyan,magenta\"",
    "build": "NODE_ENV=production next build",
    "prod:next": "NODE_ENV=production next start -p 3002",
    "prod:push": "NODE_ENV=production node scripts/push-scheduler.js",
    "lint": "eslint",
    "generate-icons": "node scripts/generate-icons.js",
    "push-scheduler": "node scripts/push-scheduler.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.5.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "next": "^16.0.7",
    "node-cron": "^4.2.1",
    "papaparse": "^5.5.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^3.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/web-push": "^3.6.4",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "nodemon": "^3.0.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
